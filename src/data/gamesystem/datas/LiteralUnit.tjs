//配列リテラルヘルパー
//
// 定数要素配列= _getLiteral( 定数配列, 行番号 );
//
function _getLiteral( val_, i_ )
{
	var hc= val_[0];
	var r= [];
	
	//成形
	for( var i=0; i<hc; i++ ) r.add(val_[(i_*hc)+i+1]);
	
	return r;
}

//
// 定数要素配列= _getLiteral( 定数配列, 検索項目, 検索対象 );
//
function _getLiteralf( val_, tx_, id_=0 )
{
	var hc= val_[0];
	var max= (val_.count-1)\hc;
	
	var ri= -1;
	
	//検索
	for( var i=0; i<max; i++ ){
		var ss= val_[(i*hc)+id_+1];
		if( tx_==ss ){ ri=(i*hc)+1; break; }
	}
	
	var r= void;
	
	//成形
	if(ri>-1){
		r= [];
		for( var i=0; i<hc; i++ ) r.add(val_[ri+i]);
	}
	
	return r;
}

